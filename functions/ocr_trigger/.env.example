# local | gcp
APP_ENV=local
LOG_LEVEL=INFO

# GCP
GCP_PROJECT_ID=deep-book-ocr

# Document AI
PROCESSOR_LOCATION=us
# どちらもOK:
#  - processor idのみ: 1234567890abcdef
#  - フルリソース名: projects/.../locations/.../processors/...
PROCESSOR_ID=REPLACE_ME
# Document AI のジョブ投入API待機秒数（長すぎるとHTTP 504の原因になり得る）
DOCAI_SUBMIT_TIMEOUT_SEC=20

# Document AI 出力先（gs:// あり/なし どちらでもOK）
TEMP_BUCKET=gs://deep-book-ocr-temp-xxxxxx

# ローカル起動で投げる入力（実GCSに置いたPDF）
LOCAL_INPUT_BUCKET=deep-book-ocr-input-xxxxxx
LOCAL_INPUT_OBJECT=uploads/sample.pdf
